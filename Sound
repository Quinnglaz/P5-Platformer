let sound;

function preload() {
  // Load the sound file
  sound = loadSound('jump.ogg');
}

function setup() {
  createCanvas(400, 400);
  // Add a button to trigger the sound
  let button = UP_ARROW('Play Sound');
  button.mousePressed(playSound);
}

function playSound() {
  // Play the loaded sound
  sound.play();
}
